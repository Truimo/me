---
import type {DiaryItem} from '@/libs/notion';
import DateTime from '@/components/diary/DateTime.astro';
import NotionRender from '@/components/notion/NotionRender.astro';

interface Props {
    diary: DiaryItem;
}

const { diary } = Astro.props;
---
<article>
    <NotionRender id={diary.id} class='my-3' />
    <DateTime date={diary.date} />
</article>
