---
import type {DiaryItem} from '@/libs/notion';
import PostCard from './PostCard.astro';

interface Props {
    list: DiaryItem[]
}

const { list } = Astro.props;
---
<div class="grid grid-cols-1 gap-6 select-none">
    {list.map((item: DiaryItem) => (
        <PostCard diary={item} />
    ))}
</div>
